class Solution {
    public int findMiddleIndex(int[] nums) {
        int total = 0;
        for(int i=0; i<nums.length; i++){
            total += nums[i];
        }
        int leftsum = 0;
        for(int j= 0; j<nums.length;j++){
            if(2 * leftsum + nums[j] == total){
                return j;
            }
            leftsum  += nums[j];
        }
        return -1;
    }
}
