# fitness on codechef
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int t = scanner.nextInt();
        
        while (t-- > 0) {
            int x = scanner.nextInt();
            // Your code goes here
            int ans = 2 * x * 5;
            System.out.println(ans);
        }
    }
}
